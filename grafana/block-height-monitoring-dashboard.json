          "hide": false,
          "query": "import \"strings\"\n\nChains = from(bucket: \"blockheights\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"block_height_request\")\n  |> filter(fn: (r) => r.chain == \"${chain_name}\")\n  |> filter(fn: (r) => r[\"_field\"] == \"block_height\")\n  |> group(columns: [\"url\"])\n  |> truncateTimeColumn(unit: 1s)\n  |> keep(columns: [\"_time\", \"_value\", \"url\"])\n  |> map(fn: (r) => ({r with url: strings.trimPrefix(v: r.url, prefix: \"http://\")}))\n  |> map(fn: (r) => ({r with url: strings.trimPrefix(v: r.url, prefix: \"https://\")}))\n  |> map(fn: (r) => ({r with url: strings.trimPrefix(v: r.url, prefix: \"wss://\")}))\n  |> yield(name: \"Chains\")",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB-BLOCKHEIGHTS}"
          },
          "hide": true,
          "query": "MaxHeight = from(bucket: \"blockheights\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"block_height_request\")\n  |> filter(fn: (r) => r.chain == \"${chain_name}\")\n  |> filter(fn: (r) => r[\"_field\"] == \"block_height\")\n  |> truncateTimeColumn(unit: 1s)\n  |> group()\n  |> window(every: 1s)\n  |> max()\n  |> window(every: inf)\n  |> drop(columns: [\"_field\", \"_measurement\", \"api\", \"chain\"])\n  |> rename(columns: {_time: \"Time\"})\n  |> yield(name: \"MaxHeight\")\n\nChains = from(bucket: \"blockheights\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"block_height_request\")\n  |> filter(fn: (r) => r.chain == \"Binance Smart Chain mainnet\")\n  |> filter(fn: (r) => r[\"_field\"] == \"block_height\")\n  |> truncateTimeColumn(unit: 1s)\n  //|> map(fn: (r) => ({ r with _value: r._value - maxRow(r: r) }))\n  //|> yield(name: \"Chains\")\n\n//union(tables: [MaxHeight, Chains])\n  //|> yield()\n",
          "refId": "A"
        }
